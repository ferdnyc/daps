    <sect2 xml:id="images.formats.dia">
     <title>DIA</title>
     <para/>
     <itemizedlist>
      <listitem>
       <para>
        Source: DIA
       </para>
      </listitem>
      <listitem>
       <para>
        Result (fo): SVG or PDF
       </para>
      </listitem>
      <listitem>
       <para>
        Result (html): PNG
       </para>
      </listitem>
     </itemizedlist>
     <figure>
      <title>DIA -> SVG (fo)/PNG (html)</title>
      <mediaobject>
       <imageobject role="fo">
        <imagedata fileref="dia_example.svg" width="35%"/>
       </imageobject>
       <imageobject role="html">
        <imagedata fileref="dia_example.png" width="35%"/>
       </imageobject>
      </mediaobject>
     </figure>
     <figure>
      <title>DIA -> PDF (fo)/PNG (html)</title>
